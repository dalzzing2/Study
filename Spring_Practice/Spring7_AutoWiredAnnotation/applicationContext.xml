<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:context="http://www.springframework.org/schema/context"
 xsi:schemaLocation="http://www.springframework.org/schema/beans 
 http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
 http://www.springframework.org/schema/context
 http://www.springframework.org/schema/context/spring-context-4.1.xsd">
 <!-- auto wire annotation을 위한 설정 -->
 <context:annotation-config/>
 <bean id="data1" class="bean.DataBean">
 	<property name="msg" value="안녕하세요"/>
 </bean>
 
 <bean id="test1" class="bean.TestBean">
 	<property name="data1" value="200"/>
 </bean>
 
 <bean id="obj1" class="bean.DataBean">
 	<property name="msg" value="반갑습니다"/>
 </bean>
</beans>











